# Database (local dev; Docker overrides this automatically)
DATABASE_URL="postgresql://dashboard:dashboard@localhost:5432/dashboard"

# Docker PostgreSQL password
POSTGRES_PASSWORD=dashboard

# Auth.js
NEXTAUTH_SECRET="generate-with-openssl-rand-base64-32"
NEXTAUTH_URL="http://localhost:3000"

# Auth providers (both enabled by default)
ENABLE_GITHUB=true
ENABLE_GUEST=true

# GitHub OAuth (required only if ENABLE_GITHUB=true)
GITHUB_CLIENT_ID=""
GITHUB_CLIENT_SECRET=""
